{
  "name": "@tokencap/proxy",
  "version": "0.1.0",
  "description": "LLM proxy server with pre-execution cost prediction and budget enforcement",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "openai",
    "anthropic",
    "proxy",
    "cost",
    "budget",
    "tokens"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.7.0",
    "hono": "^4.6.14",
    "tiktoken": "^1.0.18"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  }
}
